{
    "ID": 55925,
    "name": "Nitro-Lift Technologies, LLC v. Howard",
    "href": "https:\/\/api.oyez.org\/cases\/2012\/11-1377",
    "docket_number": "11-1377",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
