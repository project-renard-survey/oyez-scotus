                        "byte_start": 1677145,
                        "byte_stop": 1703058,
                        "speaker": {
                            "ID": 44861,
                            "name": "William H. Henderson",
                            "href": "https:\/\/api.oyez.org\/people\/william_h_henderson",
                            "last_name": "Henderson",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "william_h_henderson"
                        },
                        "text_blocks": [
                            {
                                "start": 393.415,
                                "stop": 394.206,
                                "byte_start": 1677145,
                                "byte_stop": 1680280,
                                "text": "All, no."
                            },
                            {
                                "start": 394.206,
                                "stop": 399.904,
                                "byte_start": 1680280,
                                "byte_stop": 1703058,
                                "text": "Your Honor that would include oranges and lemons or any citrus fruit."
                            }
                        ]
                    },
