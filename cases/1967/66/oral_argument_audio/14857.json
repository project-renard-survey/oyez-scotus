                        "byte_start": 1703058,
                        "byte_stop": 1717060,
                        "speaker": {
                            "ID": 15061,
                            "name": "Abe Fortas",
                            "href": "https:\/\/api.oyez.org\/people\/abe_fortas",
                            "last_name": "Fortas",
                            "roles": [
