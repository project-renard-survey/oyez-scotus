    "transcript": {
        "title": "National Labor Relations Board v. Fant Milling Company",
        "duration": 1416.335,
        "sections": [
            {
                "start": 0,
                "stop": 1416.335,
                "byte_start": 103424,
                "byte_stop": 5768756,
                "turns": [
                    {
                        "start": 0,
                        "stop": 8.918,
                        "byte_start": 103424,
                        "byte_stop": 139160,
                        "speaker": {
                            "ID": 15099,
                            "name": "Earl Warren",
                            "href": "https:\/\/api.oyez.org\/people\/earl_warren",
                            "last_name": "Warren",
                            "roles": [
                                {
                                    "id": 2743,
                                    "type": "scotus_justice",
                                    "date_start": -512503200,
                                    "date_end": -16570800,
                                    "appointing_president": "Dwight D. Eisenhower",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2743"
                                }
                            ],
                            "thumbnail": {
                                "id": 32704,
                                "mime": "image\/png",
                                "size": 50444,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/earl_warren.png"
