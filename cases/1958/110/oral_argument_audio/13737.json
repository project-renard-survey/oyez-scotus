                        },
                        "text_blocks": [
                            {
                                "start": 474.598,
                                "stop": 475.876,
                                "byte_start": 2001899,
                                "byte_stop": 2007019,
                                "text": "We\u0027ll recess now."
                            }
                        ]
                    },
                    {
                        "start": 475.876,
                        "stop": 478.651,
                        "byte_start": 2007019,
                        "byte_stop": 2017991,
                        "speaker": {
                            "ID": 45434,
                            "name": "William B. Mahoney",
                            "href": "https:\/\/api.oyez.org\/people\/william_b_mahoney",
                            "last_name": "Mahoney",
                            "roles": null,
