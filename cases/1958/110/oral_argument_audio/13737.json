    "transcript": {
        "title": "Tak Shan Fong v. United States",
        "duration": 478.651,
        "sections": [
            {
                "start": 0,
                "stop": 478.651,
                "byte_start": 103424,
                "byte_stop": 2017991,
                "turns": [
                    {
                        "start": 0,
                        "stop": 3.543,
                        "byte_start": 103424,
                        "byte_stop": 117635,
                        "speaker": {
                            "ID": 15099,
                            "name": "Earl Warren",
                            "href": "https:\/\/api.oyez.org\/people\/earl_warren",
                            "last_name": "Warren",
                            "roles": [
                                {
                                    "id": 2743,
                                    "type": "scotus_justice",
