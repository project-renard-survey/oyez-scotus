                        "text_blocks": [
                            {
                                "start": 329.406,
                                "stop": 333.183,
                                "byte_start": 1421145,
                                "byte_stop": 1436191,
                                "text": "You also assume that he was present and wasn\u0027t ill and --"
                            }
                        ]
                    },
                    {
                        "start": 333.183,
                        "stop": 333.999,
                        "byte_start": 1436191,
                        "byte_stop": 1439431,
                        "speaker": {
                            "ID": 15044,
                            "name": "Arthur J. Goldberg",
                            "href": "https:\/\/api.oyez.org\/people\/arthur_j_goldberg",
                            "last_name": "Goldberg",
                            "roles": [
                                {
                                    "id": 2688,
                                    "type": "scotus_justice",
                                    "date_start": -228855600,
                                    "date_end": -140036400,
                                    "appointing_president": "John F. Kennedy",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2688"
                                }
                            ],
                            "thumbnail": {
