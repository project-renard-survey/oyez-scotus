                        "byte_stop": 1436191,
                        "speaker": {
                            "ID": 38003,
                            "name": "Philip B. Heymann",
                            "href": "https:\/\/api.oyez.org\/people\/philip_b_heymann",
                            "last_name": "Heymann",
