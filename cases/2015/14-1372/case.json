{
    "ID": 62637,
    "name": "White v. Wheeler",
    "href": "https:\/\/api.oyez.org\/cases\/2015\/14-1372",
    "docket_number": "14-1372",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
    "first_party": "Randy White, Warden",
