                        "text_blocks": [
                            {
                                "start": 822.196,
                                "stop": 830.219,
                                "byte_start": 3389348,
                                "byte_stop": 3421427,
                                "text": "I wish to add that Mr. Justice White has also filed a dissenting opinion which Mr. Justice Blackmun and Mr. Justice Rehnquist have joined."
                            },
                            {
                                "start": 830.219,
                                "stop": 839.367,
                                "byte_start": 3421427,
                                "byte_stop": 3458103,
                                "text": "And that Mr. Justice Blackmun has filed a dissenting opinion which Mr. Justice White and Mr. Justice Rehnquist have joined."
                            }
                        ]
                    },
                    {
                        "start": 839.367,
                        "stop": 841.244,
                        "byte_start": 3458103,
                        "byte_stop": 3465417,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
                                {
                                    "id": 2717,
                                    "type": "scotus_justice",
                                    "date_start": -16570800,
                                    "date_end": 528094800,
                                    "appointing_president": "Richard Nixon",
