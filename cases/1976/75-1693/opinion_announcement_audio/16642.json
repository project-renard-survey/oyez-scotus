                        "text_blocks": [
                            {
                                "start": 10.151,
                                "stop": 18.195,
                                "byte_start": 141182,
                                "byte_stop": 173364,
                                "text": "This case is here by way of writ of certiorari granted to the United States Court of Appeals for the Fourth Circuit."
                            },
                            {
                                "start": 18.195,
                                "stop": 29.566,
                                "byte_start": 173364,
                                "byte_stop": 218817,
                                "text": "The respondent, Gary Darrell Allison, an inmate of a North Carolina Penitentiary, petitioned to Federal District Court for a writ of habeas corpus."
                            },
                            {
                                "start": 29.566,
                                "stop": 40.246,
                                "byte_start": 218817,
                                "byte_stop": 261554,
                                "text": "The Court dismissed his petition without a hearing and the Court of Appeals reversed, ruling that in the circumstances of this case, summary dismissal was improper."
                            },
                            {
                                "start": 40.246,
                                "stop": 44.691,
                                "byte_start": 261554,
                                "byte_stop": 279317,
                                "text": "We granted certiorari to review the judgment of the Court of Appeals."
                            },
                            {
                                "start": 44.691,
                                "stop": 53.073,
                                "byte_start": 279317,
                                "byte_stop": 312858,
                                "text": "For the reason set out in detail in the Court\u0027s opinion, filed today with the clerk, we affirmed the judgment of the Court of Appeals."
                            },
                            {
                                "start": 53.073,
                                "stop": 61.744,
                                "byte_start": 312858,
                                "byte_stop": 347549,
                                "text": "The Chief Justice concurs only in the judgment and Mr. Justice Rehnquist took no part in the consideration or decision of the case."
                            }
                        ]
                    },
