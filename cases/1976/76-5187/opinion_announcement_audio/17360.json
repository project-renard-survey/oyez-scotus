                            "length_of_service": 5649,
                            "identifier": "lewis_f_powell_jr"
                        },
                        "text_blocks": [
                            {
                                "start": 10.4,
                                "stop": 21.231,
                                "byte_start": 142225,
                                "byte_stop": 185484,
                                "text": "The petitioner in this case was a child of stealing two wallets from the blind operator of a newsstand in the lobby of the United States Post Office."
                            },
                            {
                                "start": 21.231,
                                "stop": 33.452,
                                "byte_start": 185484,
                                "byte_stop": 234385,
                                "text": "During his first trial, the District Court granted petitioner\u0027s motion to dismiss on the ground that the indictment had failed to specify accurately the crime charged."
                            },
                            {
                                "start": 33.452,
                                "stop": 36.226,
                                "byte_start": 234385,
                                "byte_stop": 245461,
                                "text": "Petitioner was retried and convicted."
                            },
                            {
                                "start": 36.226,
                                "stop": 40.697,
                                "byte_start": 245461,
                                "byte_stop": 263329,
                                "text": "His conviction was affirmed by the Court of Appeals for the Seventh Circuit."
                            },
                            {
                                "start": 40.697,
                                "stop": 49.31,
                                "byte_start": 263329,
                                "byte_stop": 297811,
                                "text": "We granted certiorari to consider whether the petition of second trial violated the Double Jeopardy Clause of the Fifth Amendment."
                            },
                            {
                                "start": 49.31,
                                "stop": 51.429,
                                "byte_start": 297811,
                                "byte_stop": 306274,
                                "text": "We hold that it did not."
                            },
                            {
                                "start": 51.429,
                                "stop": 59.623,
                                "byte_start": 306274,
                                "byte_stop": 339084,
                                "text": "We think the principles explained in our decision in the last term in the United States against Dinitz are applicable."
                            },
                            {
                                "start": 59.623,
                                "stop": 76.863,
                                "byte_start": 339084,
                                "byte_stop": 408047,
                                "text": "As the dismissal was granted at petitioner\u0027s request without judicial or prosecutorial overreaching and on -- and on the ground consistent with retrial, the Double Jeopardy Clause does not bought re-prosecution."
                            },
                            {
                                "start": 76.863,
                                "stop": 83.236,
                                "byte_start": 408047,
                                "byte_stop": 433543,
                                "text": "Mr. Justice Brennan and Mr. Justice Rehnquist have filed separate concurring opinions."
                            },
                            {
                                "start": 83.236,
                                "stop": 88.257,
                                "byte_start": 433543,
                                "byte_stop": 453605,
                                "text": "Mr. Justice Marshall has filed a dissenting opinion."
                            }
                        ]
                    },
