{
    "id": 23526,
    "title": "Opinion Announcement - June 19, 2014",
    "transcript": {
        "title": null,
        "duration": 202.2,
        "sections": [
            {
                "start": 0,
                "stop": 202.2,
                "byte_start": 106496,
                "byte_stop": 915247,
                "turns": [
                    {
                        "start": 0,
