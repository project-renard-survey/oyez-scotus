{
    "ID": 55997,
    "name": "Air Wisconsin Airlines Corp. v. Hoeper",
    "href": "https:\/\/api.oyez.org\/cases\/2013\/12-315",
    "docket_number": "12-315",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
