{
    "ID": 62266,
    "name": "Tolan v. Cotton",
    "href": "https:\/\/api.oyez.org\/cases\/2013\/13-551",
    "docket_number": "13-551",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
    "first_party": "Robert R. Tolan",
    "second_party": "Jeffrey Wayne Cotton",
