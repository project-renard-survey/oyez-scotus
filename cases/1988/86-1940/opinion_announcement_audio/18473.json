                        "text_blocks": [
                            {
                                "start": 7.639,
                                "stop": 13.995,
                                "byte_start": 117168,
                                "byte_stop": 142559,
                                "text": "This case is here on writ of certiorari to the Court of Appeals for the Ninth Circuit."
                            },
                            {
                                "start": 13.995,
                                "stop": 44.034,
                                "byte_start": 142559,
                                "byte_stop": 262722,
                                "text": "In an Opinion filed with the clerk today, we hold that the removal of an elected Union official in retaliation for statements he made at a Union meeting in opposition to a deuced increase sought by a Union trustee violated the Labor Management Reporting and Disclosed Act 1959."
                            },
                            {
                                "start": 44.034,
                                "stop": 47.591,
                                "byte_start": 262722,
                                "byte_stop": 276932,
                                "text": "The Court of Appeals? judgment is, thereby, affirmed."
                            },
                            {
                                "start": 47.591,
                                "stop": 51.981,
                                "byte_start": 276932,
                                "byte_stop": 294487,
                                "text": "Justice White has filed an opinion concurring in the judgment."
                            },
                            {
                                "start": 51.981,
                                "stop": 57.73,
                                "byte_start": 294487,
                                "byte_stop": 0,
                                "text": "Justice Kennedy took no part in the consideration or decision of this case."
                            }
                        ]
                    }
                ]
