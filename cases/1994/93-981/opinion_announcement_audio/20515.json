                            "identifier": "sandra_day_oconnor"
                        },
                        "text_blocks": [
                            {
                                "start": 7.308,
                                "stop": 14.435,
                                "byte_start": 116110,
                                "byte_stop": 144636,
                                "text": "This case comes to this court on writ of certiorari to the United States Court of Appeals for the Ninth Circuit."
                            },
                            {
                                "start": 14.435,
                                "stop": 20.662,
                                "byte_start": 144636,
                                "byte_stop": 169504,
                                "text": "We took the case to resolve rather a lopsided conflict among the Courts of Appeals."
                            },
                            {
                                "start": 20.662,
                                "stop": 41.658,
                                "byte_start": 169504,
                                "byte_stop": 253514,
                                "text": "In the opinion filed today we hold that the drug conspiracy statute \u00a7 21 U. S. C. \u00a7 846 does not require the government to prove that a conspirator under the Drug Conspiracy Law has committed an overt act in furtherance of the conspiracy."
                            },
                            {
                                "start": 41.658,
                                "stop": 55.732,
                                "byte_start": 253514,
                                "byte_stop": 309834,
                                "text": "Unlike the general conspiracy statute \u00a7 846 follows the common law definition of conspiracy which requires only that the conspirators enter into an agreement to commit an unlawful act."
                            },
                            {
                                "start": 55.732,
                                "stop": 59.302,
                                "byte_start": 309834,
                                "byte_stop": 324149,
                                "text": "Accordingly the judgment of the Court of Appeals is reversed."
                            },
                            {
                                "start": 59.302,
                                "stop": 61.1,
                                "byte_start": 324149,
                                "byte_stop": 0,
                                "text": "The opinion is unanimous."
                            }
                        ]
                    }
                ]
            }
        ]
    },
