    "transcript": {
        "title": "Schneble v. Florida",
        "duration": 410.672,
        "sections": [
            {
                "start": 0,
                "stop": 410.672,
                "byte_start": 73730,
                "byte_stop": 1716414,
                "turns": [
                    {
                        "start": 0,
                        "stop": 40.379,
                        "byte_start": 73730,
                        "byte_stop": 235271,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
                                {
                                    "id": 2717,
                                    "type": "scotus_justice",
                                    "date_start": -16570800,
                                    "date_end": 528094800,
                                    "appointing_president": "Richard Nixon",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2717"
                                }
                            ],
                            "thumbnail": {
