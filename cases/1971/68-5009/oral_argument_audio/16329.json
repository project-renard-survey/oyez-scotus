                            "length_of_service": 6304,
                            "identifier": "warren_e_burger"
                        },
                        "text_blocks": [
                            {
                                "start": 0,
                                "stop": 37.185,
                                "byte_start": 73730,
                                "byte_stop": 222523,
                                "text": "We will hear arguments next in Number 68-5009, Schneble against Florida."
                            },
                            {
                                "start": 37.185,
                                "stop": 40.379,
                                "byte_start": 222523,
                                "byte_stop": 235271,
                                "text": "Mr. Wells you may proceed."
                            }
                        ]
                    },
                    {
                        "start": 40.379,
                        "stop": 161.101,
                        "byte_start": 235271,
                        "byte_stop": 718223,
                        "speaker": {
                            "ID": 20359,
                            "name": "Clyde B. Wells",
                            "href": "https:\/\/api.oyez.org\/people\/clyde_b_wells",
                            "last_name": "Wells",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "clyde_b_wells"
                        },
                        "text_blocks": [
                            {
                                "start": 40.379,
                                "stop": 44.118,
                                "byte_start": 235271,
                                "byte_stop": 250213,
                                "text": "Mr. Chief Justice and may it please the Court."
                            },
                            {
                                "start": 44.118,
                                "stop": 54.17,
                                "byte_start": 250213,
                                "byte_stop": 290442,
                                "text": "This is the case of Donald Felix Schneble versus the State of Florida and I am here representing the petitioner, Donald Felix Schneble."
                            },
                            {
                                "start": 54.17,
                                "stop": 58.846,
                                "byte_start": 290442,
                                "byte_stop": 309146,
                                "text": "I would like to give you a little background on this case."
                            },
                            {
                                "start": 58.846,
                                "stop": 63.01,
                                "byte_start": 309146,
                                "byte_stop": 325864,
                                "text": "The crime was committed in July of 1965."
                            },
                            {
                                "start": 63.01,
                                "stop": 73.758,
                                "byte_start": 325864,
                                "byte_stop": 368809,
                                "text": "The defendant and his codefendant Edward Frank Snell, was picked up several days after the alleged commission of the crime."
                            },
                            {
                                "start": 73.758,
                                "stop": 79.745,
                                "byte_start": 368809,
                                "byte_stop": 392737,
                                "text": "They were lodged in the Palm Beach County Jail."
                            },
                            {
                                "start": 79.745,
                                "stop": 86.822,
                                "byte_start": 392737,
                                "byte_stop": 421054,
                                "text": "The crime allegedly occurred in Walton County, Florida, several hundred miles from where they were picked up."
                            },
                            {
                                "start": 86.822,
                                "stop": 93.265,
                                "byte_start": 421054,
                                "byte_stop": 446863,
                                "text": "They were charged initially with a traffic violation, the driver of the car, the co-defendant, Snell."
                            },
                            {
                                "start": 93.265,
                                "stop": 99.422,
                                "byte_start": 446863,
                                "byte_stop": 471523,
                                "text": "Mr. Schneble was charged with vagrancy at that time."
                            },
                            {
                                "start": 99.422,
                                "stop": 118.992,
                                "byte_start": 471523,
                                "byte_stop": 549786,
                                "text": "He was taken to Court on the morning after he was picked up in the middle of the night and sentenced for vagrancy, given a 24-hour sentence and given credit for time served which would have started his time at 5 a.m. in that morning."
                            },
                            {
                                "start": 118.992,
                                "stop": 133.984,
                                "byte_start": 549786,
                                "byte_stop": 609763,
                                "text": "During the course of the day, blood was found in the area of the front seat of the car on the right side and other damaging evidence against the defendant and his codefendant."
                            },
                            {
                                "start": 133.984,
                                "stop": 161.101,
                                "byte_start": 609763,
                                "byte_stop": 718223,
                                "text": "And so the following day some six or eight -- six, five or six hours after the defendant Schneble\u2019s sentence had expired, he was taken back before the same judge and given an additional sentence of 15 days, given the credit for time served to allow the authorities to investigate the evidence that they found in the vehicle."
                            }
                        ]
                    },
                    {
                        "start": 161.101,
                        "stop": 164.723,
                        "byte_start": 718223,
                        "byte_stop": 732643,
                        "speaker": {
                            "ID": 15043,
                            "name": "Potter Stewart",
                            "href": "https:\/\/api.oyez.org\/people\/potter_stewart",
                            "last_name": "Stewart",
                            "roles": [
                                {
                                    "id": 2687,
                                    "type": "scotus_justice",
                                    "date_start": -353962800,
                                    "date_end": 362984400,
                                    "appointing_president": "Dwight D. Eisenhower",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2687"
                                }
                            ],
                            "thumbnail": {
                                "id": 32752,
                                "mime": "image\/png",
                                "size": 56350,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/potter_stewart.png"
                            },
                            "length_of_service": 8298,
                            "identifier": "potter_stewart"
                        },
                        "text_blocks": [
                            {
                                "start": 161.101,
                                "stop": 164.723,
                                "byte_start": 718223,
                                "byte_stop": 732643,
                                "text": "That was also -- that was just a revised sentence from the vagrancy?"
                            }
                        ]
                    },
                    {
                        "start": 164.723,
                        "stop": 176.032,
                        "byte_start": 732643,
                        "byte_stop": 777887,
                        "speaker": {
                            "ID": 20359,
                            "name": "Clyde B. Wells",
                            "href": "https:\/\/api.oyez.org\/people\/clyde_b_wells",
                            "last_name": "Wells",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "clyde_b_wells"
                        },
                        "text_blocks": [
                            {
                                "start": 164.723,
                                "stop": 165.327,
                                "byte_start": 732643,
                                "byte_stop": 735046,
                                "text": "Yes sir."
                            },
                            {
                                "start": 165.327,
                                "stop": 176.032,
                                "byte_start": 735046,
                                "byte_stop": 777887,
                                "text": "It was -- the previous sentence of 24 hours was withdrawn or vacated and a new sentence was imposed after the expiration of the 24-hour sentence."
                            }
                        ]
                    },
                    {
                        "start": 176.032,
                        "stop": 177.374,
                        "byte_start": 777887,
                        "byte_stop": 783320,
                        "speaker": {
                            "ID": 15043,
                            "name": "Potter Stewart",
                            "href": "https:\/\/api.oyez.org\/people\/potter_stewart",
                            "last_name": "Stewart",
                            "roles": [
                                {
                                    "id": 2687,
                                    "type": "scotus_justice",
                                    "date_start": -353962800,
                                    "date_end": 362984400,
                                    "appointing_president": "Dwight D. Eisenhower",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2687"
                                }
                            ],
                            "thumbnail": {
                                "id": 32752,
                                "mime": "image\/png",
                                "size": 56350,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/potter_stewart.png"
                            },
                            "length_of_service": 8298,
                            "identifier": "potter_stewart"
                        },
                        "text_blocks": [
                            {
                                "start": 176.032,
                                "stop": 177.374,
                                "byte_start": 777887,
                                "byte_stop": 783320,
                                "text": "And for vagrancy?"
                            }
                        ]
                    },
                    {
                        "start": 177.374,
                        "stop": 331.385,
                        "byte_start": 783320,
                        "byte_stop": 1399288,
                        "speaker": {
                            "ID": 20359,
                            "name": "Clyde B. Wells",
                            "href": "https:\/\/api.oyez.org\/people\/clyde_b_wells",
                            "last_name": "Wells",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "clyde_b_wells"
                        },
                        "text_blocks": [
                            {
                                "start": 177.374,
                                "stop": 178.506,
                                "byte_start": 783320,
                                "byte_stop": 787813,
                                "text": "Yes sir."
                            },
                            {
                                "start": 178.506,
                                "stop": 200.02,
                                "byte_start": 787813,
                                "byte_stop": 873913,
                                "text": "Then from that point the defendant, Schneble, was taken from the Palm Beach County Jail by two detectives, Haroun (ph) and Hailey (ph) and they were his constant companions for a period of time, ranging from the testimony from 48 or 40 to 60 hours."
                            },
                            {
                                "start": 200.02,
                                "stop": 206.952,
                                "byte_start": 873913,
                                "byte_stop": 901603,
                                "text": "During that period of time the detectives Hailey (ph) and Haroun carried him to three different jails."
                            },
                            {
                                "start": 206.952,
                                "stop": 211.467,
                                "byte_start": 901603,
                                "byte_stop": 919679,
                                "text": "They carried him to an airport where they kept him for three hours and questioned him."
                            },
                            {
                                "start": 211.467,
                                "stop": 222.471,
                                "byte_start": 919679,
                                "byte_stop": 963670,
                                "text": "They had a fellow policeman come up behind him at a high rate of speed and throw firecrackers out and harassed the defendant and scared him."
                            },
                            {
                                "start": 222.471,
                                "stop": 230.956,
                                "byte_start": 963670,
                                "byte_stop": 997629,
                                "text": "They told him that or suggested to him that his codefendant Snell had put a price of $5,000.00 on his head."
                            },
                            {
                                "start": 230.956,
                                "stop": 254.841,
                                "byte_start": 997629,
                                "byte_stop": 1093132,
                                "text": "They walked out of the jail with him with a cop vest to be sure nobody would shoot him on the way out and kept him under constant harassments, surveillance for this period of 40 to 60 hours, they even put a detective in the cell next to him to keep him awake and harass him during the day time on one occasion while they rested."
                            },
                            {
                                "start": 254.841,
                                "stop": 262.42,
                                "byte_start": 1093132,
                                "byte_stop": 1123434,
                                "text": "After some 40 to 60 hours of this type of treatment, the defendant, Schneble, made a statement to him."
                            },
                            {
                                "start": 262.42,
                                "stop": 282.481,
                                "byte_start": 1123434,
                                "byte_stop": 1203683,
                                "text": "During this same period of time the defendant Snell was being questioned by other officers and he made certain limited statements against his interest and against the interest of both of them which was also introduced at the trial."
                            },
                            {
                                "start": 282.481,
                                "stop": 296.848,
                                "byte_start": 1203683,
                                "byte_stop": 1261152,
                                "text": "Now, these defendants were then brought back to Walton County, Florida after the body had been discovered as a result of Mr. Schneble\u2019s statement and there they were indicted by a grand jury."
                            },
                            {
                                "start": 296.848,
                                "stop": 322.631,
                                "byte_start": 1261152,
                                "byte_stop": 1364283,
                                "text": "The grand jury returned separate indictments against these two defendants, but upon the -- and I might add at that grand jury session, there was a non-authorized person in the grand jury room, in that a civil case was being tried at the same time and a witness in one of the civil cases wound up in the grand jury room and sat there all afternoon during the deliberation with the grand jury."
                            },
                            {
                                "start": 322.631,
                                "stop": 331.385,
                                "byte_start": 1364283,
                                "byte_stop": 1399288,
                                "text": "A hearing was held on this and the Trial Judge determined that there was nothing improper or not improper that -- so that the indictment should be quashed."
                            }
                        ]
                    },
                    {
                        "start": 331.385,
                        "stop": 338.427,
                        "byte_start": 1399288,
                        "byte_stop": 1427500,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
                                {
                                    "id": 2717,
                                    "type": "scotus_justice",
                                    "date_start": -16570800,
                                    "date_end": 528094800,
                                    "appointing_president": "Richard Nixon",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2717"
                                }
                            ],
                            "thumbnail": {
                                "id": 32773,
                                "mime": "image\/png",
                                "size": 52511,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/warren_burger.png"
                            },
                            "length_of_service": 6304,
                            "identifier": "warren_e_burger"
                        },
                        "text_blocks": [
                            {
                                "start": 331.385,
                                "stop": 331.577,
                                "byte_start": 1399288,
                                "byte_stop": 1400123,
                                "text": "What?"
                            },
                            {
                                "start": 331.577,
                                "stop": 338.427,
                                "byte_start": 1400123,
                                "byte_stop": 1427500,
                                "text": "Let us make it clear Mr. Wells, how did they come on the information as to where the body of the dead woman was located?"
                            }
                        ]
                    },
                    {
                        "start": 338.427,
                        "stop": 345.598,
                        "byte_start": 1427500,
                        "byte_stop": 1456130,
                        "speaker": {
                            "ID": 20359,
                            "name": "Clyde B. Wells",
                            "href": "https:\/\/api.oyez.org\/people\/clyde_b_wells",
                            "last_name": "Wells",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "clyde_b_wells"
                        },
                        "text_blocks": [
                            {
                                "start": 338.427,
                                "stop": 345.598,
                                "byte_start": 1427500,
                                "byte_stop": 1456130,
                                "text": "That was from the statements made by the defendant, Schneble after some 40 to 60 hours of continued --"
                            }
                        ]
                    },
                    {
                        "start": 345.598,
                        "stop": 352.194,
                        "byte_start": 1456130,
                        "byte_stop": 1482566,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
                                {
                                    "id": 2717,
                                    "type": "scotus_justice",
                                    "date_start": -16570800,
                                    "date_end": 528094800,
                                    "appointing_president": "Richard Nixon",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2717"
                                }
                            ],
                            "thumbnail": {
                                "id": 32773,
                                "mime": "image\/png",
                                "size": 52511,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/warren_burger.png"
                            },
                            "length_of_service": 6304,
                            "identifier": "warren_e_burger"
                        },
                        "text_blocks": [
                            {
                                "start": 345.598,
                                "stop": 352.194,
                                "byte_start": 1456130,
                                "byte_stop": 1482566,
                                "text": "None of those at that time had come from Snell or anyone else?"
                            }
                        ]
                    },
                    {
                        "start": 352.194,
                        "stop": 405.693,
                        "byte_start": 1482566,
                        "byte_stop": 1696561,
                        "speaker": {
                            "ID": 20359,
                            "name": "Clyde B. Wells",
                            "href": "https:\/\/api.oyez.org\/people\/clyde_b_wells",
                            "last_name": "Wells",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "clyde_b_wells"
                        },
                        "text_blocks": [
                            {
                                "start": 352.194,
                                "stop": 357.735,
                                "byte_start": 1482566,
                                "byte_stop": 1504718,
                                "text": "The only statement that had come from Snell was that he had told them the route they had taken."
                            },
                            {
                                "start": 357.735,
                                "stop": 396.937,
                                "byte_start": 1504718,
                                "byte_stop": 1661557,
                                "text": "He had told them that when they found her, she was -- well, I do not know he -- how he said he found, but the lieutenant Larry Yates (ph) had questioned him and stated that he -- that she had been sitting in the right front of the car and that Schneble, the defendant was seated in the backseat and Snell was driving and he had choked her with a cord and Snell made the statement to him that he had painted a hypothetical of actually what happened and when they found her to do not overlook the obvious, that was his statement to him."
                            },
                            {
                                "start": 396.937,
                                "stop": 405.693,
                                "byte_start": 1661557,
                                "byte_stop": 1696561,
                                "text": "He also told them the route they took when they left New Orleans on their way to Panama City and then on down to Tampa."
                            }
                        ]
                    },
                    {
                        "start": 405.693,
                        "stop": 408.037,
                        "byte_start": 1696561,
                        "byte_stop": 1705965,
                        "speaker": null,
                        "text_blocks": [
                            {
                                "start": 405.693,
                                "stop": 408.037,
                                "byte_start": 1696561,
                                "byte_stop": 1705965,
                                "text": "This was -- alright."
                            }
                        ]
                    },
                    {
                        "start": 408.037,
                        "stop": 410.672,
                        "byte_start": 1705965,
                        "byte_stop": 1716414,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
                                {
                                    "id": 2717,
                                    "type": "scotus_justice",
                                    "date_start": -16570800,
                                    "date_end": 528094800,
                                    "appointing_president": "Richard Nixon",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2717"
                                }
                            ],
                            "thumbnail": {
                                "id": 32773,
                                "mime": "image\/png",
                                "size": 52511,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/warren_burger.png"
                            },
                            "length_of_service": 6304,
                            "identifier": "warren_e_burger"
                        },
                        "text_blocks": [
                            {
                                "start": 408.037,
                                "stop": 410.672,
                                "byte_start": 1705965,
                                "byte_stop": 1716414,
                                "text": "We will resume at usual time in the morning."
                            }
                        ]
                    }
                ]
