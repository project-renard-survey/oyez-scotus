                        "text_blocks": [
                            {
                                "start": 634.404,
                                "stop": 637.578,
                                "byte_start": 2641063,
                                "byte_stop": 2653811,
                                "text": "Would they have been able to receive fees there?"
                            },
                            {
                                "start": 637.578,
                                "stop": 639.892,
                                "byte_start": 2653811,
                                "byte_stop": 2663006,
                                "text": "You can respond to that at 1:00 o'clock."
                            },
                            {
                                "start": 639.892,
                                "stop": 641.427,
                                "byte_start": 2663006,
                                "byte_stop": 2669171,
                                "text": "You can think it over in the meantime."
