                        "text_blocks": [
                            {
                                "start": 9.082,
                                "stop": 13.664,
                                "byte_start": 65034,
                                "byte_stop": 83425,
                                "text": "This case is here from the Supreme Court of Mississippi."
                            },
                            {
                                "start": 13.664,
                                "stop": 21.794,
                                "byte_start": 83425,
                                "byte_stop": 115921,
                                "text": "The Mississippi 5% sales tax is applied to the pump price of gasoline."
                            },
                            {
                                "start": 21.794,
                                "stop": 33.273,
                                "byte_start": 115921,
                                "byte_stop": 161792,
                                "text": "That pump price is increased 13 cents represent federal and state excise taxes passed on by the retailer to the motorist."
                            },
                            {
                                "start": 33.273,
                                "stop": 44.171,
                                "byte_start": 161792,
                                "byte_stop": 205364,
                                "text": "The Mississippi Supreme Court held that this does not render the sales tax an unconstitutional tax upon a tax."
                            },
                            {
                                "start": 44.171,
                                "stop": 46.303,
                                "byte_start": 205364,
                                "byte_stop": 213932,
                                "text": "We agree and affirm."
                            },
                            {
                                "start": 46.303,
                                "stop": 60.051,
                                "byte_start": 213932,
                                "byte_stop": 268790,
                                "text": "Mr. Justice Douglas took no part in the consideration or decision of the case."
                            }
                        ]
                    }
                ]
            }
