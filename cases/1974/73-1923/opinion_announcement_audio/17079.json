                        "text_blocks": [
                            {
                                "start": 0,
                                "stop": 10.009,
                                "byte_start": 28672,
                                "byte_stop": 68796,
                                "text": "I have the opinion and judgment of the Court to announce in 73-1923, Eastland against United States Servicemen\u0027s Fund."
                            },
                            {
                                "start": 10.009,
                                "stop": 19.865,
                                "byte_start": 68796,
                                "byte_stop": 108189,
                                "text": "And for reasons stated in an opinion filed today with the clerk, the judgment of the Court of Appeals with respect to the Senate aspect of the case is reversed."
                            },
                            {
                                "start": 19.865,
                                "stop": 26.436,
                                "byte_start": 108189,
                                "byte_stop": 134520,
                                "text": "The case is remanded by the Court of Appeals for the entry of a judgment directing the District Court to dismiss the complaint."
                            },
                            {
                                "start": 26.436,
                                "stop": 36.87,
                                "byte_start": 134520,
                                "byte_stop": 176212,
                                "text": "The House aspects of this case are remanded with directions to return to the District Court for further consideration consistent with this opinion."
                            },
                            {
                                "start": 36.87,
                                "stop": 44.551,
                                "byte_start": 176212,
                                "byte_stop": 206932,
                                "text": "Mr. Justice Marshall with whom Mr. Justice Brennan and Mr. Justice Stewart joined filed an opinion concurring in the judgment."
                            },
                            {
                                "start": 44.551,
                                "stop": 47.814,
                                "byte_start": 206932,
                                "byte_stop": 219888,
                                "text": "Mr. Justice Douglas has filed a dissenting opinion."
                            }
                        ]
                    }
                ]
            }
        ]
