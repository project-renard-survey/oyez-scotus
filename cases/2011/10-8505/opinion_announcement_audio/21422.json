                        "text_blocks": [
                            {
                                "start": 5.879,
                                "stop": 13.15,
                                "byte_start": 129087,
                                "byte_stop": 158135,
                                "text": "The first case is Williams versus Illinois, Number 10-8505."
                            },
                            {
                                "start": 13.15,
                                "stop": 18.344,
                                "byte_start": 158135,
                                "byte_stop": 178928,
                                "text": "This case involves DNA evidence and the Confrontation Clause of the Sixth Amendment."
                            },
                            {
                                "start": 18.344,
                                "stop": 26.792,
                                "byte_start": 178928,
                                "byte_stop": 212679,
                                "text": "The petitioner, Sandy Williams was convicted in state court in Illinois for abducting, raping, and robbing a young woman."
                            },
                            {
                                "start": 26.792,
                                "stop": 39.774,
                                "byte_start": 212679,
                                "byte_stop": 264610,
                                "text": "At his trial, the victim identified Williams as her attacker and the State also attempted to show that Williams\u0027 DNA matched the forensic sample obtained from the victim at the hospital after the rape."
                            },
                            {
                                "start": 39.774,
                                "stop": 45.3,
                                "byte_start": 264610,
                                "byte_stop": 286762,
                                "text": "Williams subjected to the testimony by the DNA expert who testified for the State."
                            },
                            {
                                "start": 45.3,
                                "stop": 54.347,
                                "byte_start": 286762,
                                "byte_stop": 322915,
                                "text": "After petitioner was convicted, he filed an appeal claiming that the DNA expert\u0027s testimony violated the Confrontation Clause."
                            },
                            {
                                "start": 54.347,
                                "stop": 60.644,
                                "byte_start": 322915,
                                "byte_stop": 348097,
                                "text": "The State Court of Appeals rejected this argument and the Supreme Court of Illinois affirmed."
                            },
                            {
                                "start": 60.644,
                                "stop": 63.729,
                                "byte_start": 348097,
                                "byte_stop": 360427,
                                "text": "We granted certiorari and we now affirm."
                            },
                            {
                                "start": 63.729,
                                "stop": 69.398,
                                "byte_start": 360427,
                                "byte_stop": 383101,
                                "text": "Anyone interested in understanding the Court\u0027s holding will have to read our opinions."
                            },
                            {
                                "start": 69.398,
                                "stop": 74.235,
                                "byte_start": 383101,
                                "byte_stop": 402432,
                                "text": "I have filed an opinion in which the Chief Justice, Justice Kennedy, and Justice Breyer join."
                            },
                            {
                                "start": 74.235,
                                "stop": 77.321,
                                "byte_start": 402432,
                                "byte_stop": 414762,
                                "text": "Justice Breyer has filed a concurring opinion."
                            },
                            {
                                "start": 77.321,
                                "stop": 81.659,
                                "byte_start": 414762,
                                "byte_stop": 432212,
                                "text": "Justice Thomas has filed an opinion concurring in the judgment."
                            },
                            {
                                "start": 81.659,
                                "stop": 89.992,
                                "byte_start": 432212,
                                "byte_stop": 0,
                                "text": "Justice Kagan has filed a dissenting opinion in which Justices Scalia and Ginsburg and Sotomayor have joined."
                            }
                        ]
                    }
                ]
            }
