                        "text_blocks": [
                            {
                                "start": 0,
                                "stop": 5.118,
                                "byte_start": 79982,
                                "byte_stop": 100462,
                                "text": "I have the judgment to announce in McDaniel against Paty."
                            },
                            {
                                "start": 5.118,
                                "stop": 16.876,
                                "byte_start": 100462,
                                "byte_stop": 147587,
                                "text": "The Tennessee Supreme Court held that under the constitutional laws of Tennessee or Ministers of the Gospel are barred from certain public offices."
                            },
                            {
                                "start": 16.876,
                                "stop": 30.623,
                                "byte_start": 147587,
                                "byte_stop": 202549,
                                "text": "For reasons stated in an opinion filed with the clerk this morning, the judgment of the Tennessee Supreme Court is reversed and the case is remanded to that court for further proceedings, not inconsistent with the opinion filed this morning."
                            },
                            {
                                "start": 30.623,
                                "stop": 38.13,
                                "byte_start": 202549,
                                "byte_stop": 232537,
                                "text": "Mr. Justice Brennan with whom Mr. Justice Marshall joined, filed an opinion concurring in the judgment on different grounds."
                            },
                            {
                                "start": 38.13,
                                "stop": 45.845,
                                "byte_start": 232537,
                                "byte_stop": 263466,
                                "text": "Mr. Justice Stewart and Mr. Justice White also filed separate opinions concurring in the judgment on different grounds."
                            },
                            {
                                "start": 45.845,
                                "stop": 52.59,
                                "byte_start": 263466,
                                "byte_stop": 290320,
                                "text": "Mr. Justice Blackmun took no part in the consideration or decisions of this case."
                            }
                        ]
                    }
                ]
            }
        ]
