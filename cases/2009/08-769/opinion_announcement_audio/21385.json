                        "text_blocks": [
                            {
                                "start": 8.443,
                                "stop": 14.848,
                                "byte_start": 62527,
                                "byte_stop": 88127,
                                "text": "This case comes to us on writ of certiorari to the United States Court of Appeals for the Second Circuit."
                            },
                            {
                                "start": 14.848,
                                "stop": 36.339,
                                "byte_start": 88127,
                                "byte_stop": 174122,
                                "text": "It involves complicated arbitration issues and so for present purposes, it suffices to say that we hold that imposing class arbitration on parties who have agreed to arbitrate their disputes but have not agreed to authorize class arbitration is not consistent with the Federal Arbitration Act."
                            },
                            {
                                "start": 36.339,
                                "stop": 39.155,
                                "byte_start": 174122,
                                "byte_stop": 185302,
                                "text": "We therefore reverse and remand."
                            },
                            {
                                "start": 39.155,
                                "stop": 44.956,
                                "byte_start": 185302,
                                "byte_stop": 208499,
                                "text": "Justice Ginsburg has filed a dissenting opinion in which Justices Stevens and Breyer have joined."
                            },
                            {
                                "start": 44.956,
                                "stop": 50.26,
                                "byte_start": 208499,
                                "byte_stop": 229710,
                                "text": "Justice Sotomayor took no part in the consideration or decision of this case."
                            }
                        ]
                    }
                ]
            }
