                                "start": 6.582,
                                "stop": 17.65,
                                "byte_start": 117181,
                                "byte_stop": 161485,
                                "text": "This case is before us on exceptions that were filed to the Special Master\u2019s report which was filed on this original action."
                            },
                            {
                                "start": 17.65,
                                "stop": 26.051,
                                "byte_start": 161485,
                                "byte_stop": 195131,
                                "text": "For reasons stated in an opinion of file with the Court, the recommendations of the Special Master are adapted in part."
                            },
                            {
                                "start": 26.051,
                                "stop": 29.094,
                                "byte_start": 195131,
                                "byte_stop": 207252,
                                "text": "The exceptions filed by Wyoming are sustained."
                            },
                            {
                                "start": 29.094,
                                "stop": 33.027,
                                "byte_start": 207252,
                                "byte_stop": 223030,
                                "text": "The exceptions filed by Oklahoma are rejected."
                            },
                            {
                                "start": 33.027,
                                "stop": 37.25,
                                "byte_start": 223030,
                                "byte_stop": 239852,
                                "text": "Wyoming\u2019s motion for summary judgment is granted."
                            },
                            {
                                "start": 37.25,
                                "stop": 40.835,
                                "byte_start": 239852,
                                "byte_stop": 254272,
                                "text": "Oklahoma\u2019s motion for summary judgment is denied."
                            },
                            {
                                "start": 40.835,
                                "stop": 54.727,
                                "byte_start": 254272,
                                "byte_stop": 0,
                                "text": "Justice Scalia has filed a dissenting opinion in which the Chief Justice and Justice Thomas have joined; Justice Thomas has also filed a dissenting opinion, and the Chief Justice and Justice Scalia have joined that opinion."
                            }
                        ]
                    }
                ]
            }
