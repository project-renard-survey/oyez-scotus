                        "text_blocks": [
                            {
                                "start": 10.538,
                                "stop": 17.824,
                                "byte_start": 133993,
                                "byte_stop": 163146,
                                "text": "In this case which comes to us on appeal from the United States Court of Appeals for the Eleventh Circuit."
                            },
                            {
                                "start": 17.824,
                                "stop": 26.143,
                                "byte_start": 163146,
                                "byte_stop": 196373,
                                "text": "The Court of Appeals in that case held that the Pole Attachments ACT of the state to be unconstitutional."
                            },
                            {
                                "start": 26.143,
                                "stop": 42.525,
                                "byte_start": 196373,
                                "byte_stop": 261888,
                                "text": "That Act provides that the Federal Communications Commission may regulate the rates which elected power utilities charge, cable television operators wishing to connect transmission cable to the utility poles."
                            },
                            {
                                "start": 42.525,
                                "stop": 46.036,
                                "byte_start": 261888,
                                "byte_stop": 275995,
                                "text": "We reverse the decision of the Court of Appeals."
                            },
                            {
                                "start": 46.036,
                                "stop": 53.159,
                                "byte_start": 275995,
                                "byte_stop": 0,
                                "text": "Justice Powell has filed a concurring opinion in which Justices O\u0027Connor joins."
                            }
                        ]
                    }
                ]
            }
