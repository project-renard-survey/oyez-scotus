                        "text_blocks": [
                            {
                                "start": 392.261,
                                "stop": 401.217,
                                "byte_start": 1672547,
                                "byte_stop": 1708387,
                                "text": "There is not Mr. Justice Frankfurter, if you accept the fact that the agents went through a meaningless act in front of the Commissioner."
                            }
                        ]
                    },
                    {
                        "start": 401.217,
                        "stop": 418.385,
                        "byte_start": 1708387,
                        "byte_stop": 1777037,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
