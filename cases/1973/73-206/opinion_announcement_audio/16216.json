{
    "id": 16216,
    "title": "Opinion Announcement - June 19, 1974",
    "transcript": {
        "title": "Parker v. Levy",
        "duration": 825.649,
        "sections": [
            {
                "start": 0,
                "stop": 825.649,
                "byte_start": 76800,
                "byte_stop": 3379304,
                "turns": [
                    {
                        "start": 0,
                        "stop": 13.891,
                        "byte_start": 76800,
                        "byte_stop": 132389,
                        "speaker": {
                            "ID": 15073,
                            "name": "Warren E. Burger",
                            "href": "https:\/\/api.oyez.org\/people\/warren_e_burger",
                            "last_name": "Burger",
                            "roles": [
