{
    "ID": 62388,
    "name": "Chicago, Milwaukee and St. Paul Railway Company v. Minnesota",
    "href": "https:\/\/api.oyez.org\/cases\/1850-1900\/134us418",
    "docket_number": "762",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
