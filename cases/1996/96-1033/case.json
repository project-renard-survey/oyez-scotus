{
    "ID": 54557,
    "name": "Lords Landing Village Condominium Council of Unit Owners v. Continental Insurance Company",
    "href": "https:\/\/api.oyez.org\/cases\/1996\/96-1033",
    "docket_number": "96-1033",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
    "first_party": "Lords Landing Village Condominium Council of Unit Owners",
