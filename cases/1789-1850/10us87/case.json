{
    "ID": 62333,
    "name": "Fletcher v. Peck",
    "href": "https:\/\/api.oyez.org\/cases\/1789-1850\/10us87",
    "docket_number": "None",
    "additional_docket_numbers": null,
    "manner_of_jurisdiction": "Writ of \u003Ci\u003Ecertiorari\u003C\/i\u003E",
