                                "start": 8.947,
                                "stop": 18.228,
                                "byte_start": 108846,
                                "byte_stop": 145940,
                                "text": "This case involves a respective rights of New Mexico, Texas and Oklahoma to the waters of the Canadian River."
                            },
                            {
                                "start": 18.228,
                                "stop": 20.644,
                                "byte_start": 145940,
                                "byte_stop": 155657,
                                "text": "The Court pointed a Special Master."
                            },
                            {
                                "start": 20.644,
                                "stop": 28.301,
                                "byte_start": 155657,
                                "byte_stop": 186273,
                                "text": "The Special Master has filed a report and the States have filed exceptions to that report."
                            },
                            {
                                "start": 28.301,
                                "stop": 34.999,
                                "byte_start": 186273,
                                "byte_stop": 213022,
                                "text": "The case was argued and we now sustain the exceptions in part and overrule the exceptions in part."
                            },
                            {
                                "start": 34.999,
                                "stop": 48.527,
                                "byte_start": 213022,
                                "byte_stop": 267044,
                                "text": "And the case has remanded to the Special Master for further proceedings, just the Chief Justice has filed an opinion concurring in part and dissenting in part and Justice\u0027s O\u0027Connor, Scalia and Kennedy have joined the Chief Justice."
                            }
                        ]
                    }
