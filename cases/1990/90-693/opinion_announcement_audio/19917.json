                        "text_blocks": [
                            {
                                "start": 8.903,
                                "stop": 15.463,
                                "byte_start": 109595,
                                "byte_stop": 135822,
                                "text": "This case is here on certiorari to the United States Court of Appeals for the Tenth Circuit."
                            },
                            {
                                "start": 15.463,
                                "stop": 30.137,
                                "byte_start": 135822,
                                "byte_stop": 194545,
                                "text": "The issue is whether a mortgage lien that passes through the Chapter 7 Bankruptcy is a claim subject to inclusion in the Chapter 13 reorganization plan."
                            },
                            {
                                "start": 30.137,
                                "stop": 39.653,
                                "byte_start": 194545,
                                "byte_stop": 232580,
                                "text": "In an opinion filed with the Clerk today, we hold that a mortgage lien, in such circumstances, is a claim under the Bankruptcy Code."
                            },
                            {
                                "start": 39.653,
                                "stop": 44.095,
                                "byte_start": 232580,
                                "byte_stop": 0,
                                "text": "We, therefore, reverse the judgment of the Tenth Circuit."
                            }
                        ]
                    }
                ]
