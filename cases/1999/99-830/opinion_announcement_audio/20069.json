    "title": "Opinion Announcement - June 28, 2000",
    "transcript": {
        "title": "Stenberg v. Carhart",
        "duration": 924.918,
        "sections": [
            {
                "start": 0,
                "stop": 924.918,
                "byte_start": 84423,
                "byte_stop": 0,
                "turns": [
                    {
                        "start": 0,
