                        "text_blocks": [
                            {
                                "start": 0,
                                "stop": 6.832,
                                "byte_start": 28672,
                                "byte_stop": 56048,
                                "text": "In the EEOC case is here on certiorari to the Court of Appeals for the Tenth Circuit."
                            },
                            {
                                "start": 6.832,
                                "stop": 26.934,
                                "byte_start": 56048,
                                "byte_stop": 136505,
                                "text": "In that case, we hold that a state agency\u0027s waiver of the 60-day federal period provided by Title VII constitutes a termination of state proceedings so that the EEOC may immediately began to process the charge of discrimination."
                            },
                            {
                                "start": 26.934,
                                "stop": 32.841,
                                "byte_start": 136505,
                                "byte_stop": 160120,
                                "text": "Therefore, we reverse the decision of the Court of Appeals and remand the case."
                            },
                            {
                                "start": 32.841,
                                "stop": 38.533,
                                "byte_start": 160120,
                                "byte_stop": 182899,
                                "text": "Justice O\u0027Connor has filed an opinion concurring in part and concurring into judgment."
                            },
                            {
                                "start": 38.533,
                                "stop": 45.92,
                                "byte_start": 182899,
                                "byte_stop": 212365,
                                "text": "Justice Stevens has filed a dissenting opinion in which the Chief Justice and Justice Scalia have joined."
                            },
                            {
                                "start": 45.92,
                                "stop": 50.827,
                                "byte_start": 212365,
                                "byte_stop": 231905,
                                "text": "Justice Kennedy took no part in the consideration or decision of this case."
                            }
                        ]
                    }
                ]
            }
