                        "text_blocks": [
                            {
                                "start": 9.641,
                                "stop": 33.767,
                                "byte_start": 67333,
                                "byte_stop": 163777,
                                "text": "This case, Goodyear Atomic Corporation versus Miller, is here on appeal from the Supreme Court of the Ohio and it involves Ohio workmen\u0027s compensation law which provides at an additional award for injuries resulting from a violation of a state safety regulation may be considered."
                            },
                            {
                                "start": 33.767,
                                "stop": 39.478,
                                "byte_start": 163777,
                                "byte_stop": 186661,
                                "text": "We hold that because the federal law provides clear authorization."
                            },
                            {
                                "start": 39.478,
                                "stop": 48.497,
                                "byte_start": 186661,
                                "byte_stop": 222710,
                                "text": "The Supremacy Clause does not bar this application, this provision of the Ohio law to the federal facilities."
                            },
                            {
                                "start": 48.497,
                                "stop": 52.872,
                                "byte_start": 222710,
                                "byte_stop": 240264,
                                "text": "Accordingly, the judgment of the Ohio Supreme Court is affirmed."
                            },
                            {
                                "start": 52.872,
                                "stop": 58.554,
                                "byte_start": 240264,
                                "byte_stop": 262938,
                                "text": "Justice White has filed a dissenting opinion in which Justice O\u0027Connor has joined."
                            },
                            {
                                "start": 58.554,
                                "stop": 63.291,
                                "byte_start": 262938,
                                "byte_stop": 281746,
                                "text": "Justice Kennedy took no part in the consideration or decision of this case."
                            }
                        ]
                    }
                ]
            }
