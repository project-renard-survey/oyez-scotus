                                "start": 8.834,
                                "stop": 12.511,
                                "byte_start": 64094,
                                "byte_stop": 78723,
                                "text": "This case is here from the Court of Appeals for the Second Circuit."
                            },
                            {
                                "start": 12.511,
                                "stop": 23.406,
                                "byte_start": 78723,
                                "byte_stop": 122399,
                                "text": "The petitioners were convicted for violating the securities laws and the mail and wire fraud statutes."
                            },
                            {
                                "start": 23.406,
                                "stop": 28.468,
                                "byte_start": 122399,
                                "byte_stop": 142566,
                                "text": "The Court of Appeals affirmed those convictions and we granted certiorari."
                            },
                            {
                                "start": 28.468,
                                "stop": 47.694,
                                "byte_start": 142566,
                                "byte_stop": 219470,
                                "text": "We affirmed the -- all of the judgment below with respect to the securities laws by an equally divided court and for the reasons stated in an opinion filed with the clerk, we affirmed the mail and wire fraud convictions."
                            },
                            {
                                "start": 47.694,
                                "stop": 49.826,
                                "byte_start": 219470,
                                "byte_stop": 227934,
                                "text": "The opinion is unanimous."
                            }
                        ]
