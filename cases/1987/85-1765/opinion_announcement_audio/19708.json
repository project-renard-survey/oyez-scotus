                        "text_blocks": [
                            {
                                "start": 20.473,
                                "stop": 26.798,
                                "byte_start": 110592,
                                "byte_stop": 135879,
                                "text": "In the Bankers Life case is here on appeal from the Supreme Court of Mississippi."
                            },
                            {
                                "start": 26.798,
                                "stop": 41.969,
                                "byte_start": 135879,
                                "byte_stop": 196587,
                                "text": "In that case, we hold that a state statute imposing a 15% penalty on parties who appeal unsuccessfully, this money judgment does not violate the Equal Protection Clause."
                            },
                            {
                                "start": 41.969,
                                "stop": 45.051,
                                "byte_start": 196587,
                                "byte_stop": 208917,
                                "text": "We therefore affirmed the judgment below."
                            },
                            {
                                "start": 45.051,
                                "stop": 58.244,
                                "byte_start": 208917,
                                "byte_stop": 261684,
                                "text": "Appellant also challenges the substantial punitive damages award but we conclude that this challenge has not raise below and therefore, we do not reach it here."
                            },
                            {
                                "start": 58.244,
                                "stop": 63.264,
                                "byte_start": 261684,
                                "byte_stop": 281746,
                                "text": "Justice White has filed a concurring opinion in which Justice Scalia has joined."
                            },
                            {
                                "start": 63.264,
                                "stop": 70.918,
                                "byte_start": 281746,
                                "byte_stop": 312362,
                                "text": "Justice O\u0027Connor has filed an opinion concurring in part and concurring into judgment in which Justice Scalia has joined."
                            },
                            {
                                "start": 70.918,
                                "stop": 76.83,
                                "byte_start": 312362,
                                "byte_stop": 336081,
                                "text": "Justice Scalia has filed an opinion concurring in part and concurring into judgment."
                            },
                            {
                                "start": 76.83,
                                "stop": 83.096,
                                "byte_start": 336081,
                                "byte_stop": 361159,
                                "text": "Justice Blackmun has filed an opinion concurring in part and dissenting in part."
                            },
                            {
                                "start": 83.096,
                                "stop": 89.746,
                                "byte_start": 361159,
                                "byte_stop": 387594,
                                "text": "And Justice Stevens and Justice Kennedy took no part into consideration or decision."
                            }
                        ]
                    }
                ]
            }
        ]
