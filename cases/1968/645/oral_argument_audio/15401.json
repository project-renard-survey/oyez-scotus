                        "text_blocks": [
                            {
                                "start": 218.467,
                                "stop": 227.21,
                                "byte_start": 977377,
                                "byte_stop": 1012276,
                                "text": "The last of which was -- the third of which was that fingerprints are not excluded from the -- are not accepted from the exclusionary rule."
                            },
                            {
                                "start": 227.21,
                                "stop": 229.918,
                                "byte_start": 1012276,
                                "byte_stop": 1023143,
                                "text": "There's the Bynum case in the District of Columbia."
                            }
                        ]
                    },
