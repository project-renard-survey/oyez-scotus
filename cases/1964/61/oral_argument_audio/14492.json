                            }
                        ]
                    },
                    {
                        "start": 965.577,
                        "stop": 966.413,
                        "byte_start": 3892057,
                        "byte_stop": 3895400,
                        "speaker": {
                            "ID": 30662,
                            "name": "J. R. Goldthwaite, Jr.",
                            "href": "https:\/\/api.oyez.org\/people\/j_r_goldthwaite_jr",
                            "view_count": 0,
