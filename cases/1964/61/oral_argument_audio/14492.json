                    },
                    {
                        "start": 32.404,
                        "stop": 35.142,
                        "byte_start": 159368,
                        "byte_stop": 170339,
                        "speaker": {
                            "ID": 30662,
                            "name": "J. R. Goldthwaite, Jr.",
                            "href": "https:\/\/api.oyez.org\/people\/j_r_goldthwaite_jr",
