                        },
                        "text_blocks": [
                            {
                                "start": 94.95,
                                "stop": 100.616,
                                "byte_start": 408492,
                                "byte_stop": 431167,
                                "text": "Mr. Justice Blackman and Mr. Justice Stevens took no part in the consideration of the case that Mr. Justice Marshall has just announced."
                            },
                            {
                                "start": 100.616,
                                "stop": 101.953,
                                "byte_start": 431167,
                                "byte_stop": 436391,
                                "text": "Thank you, Mr. Justice Marshall."
                            }
                        ]
                    }
