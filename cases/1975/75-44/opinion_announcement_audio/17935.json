                                "start": 0,
                                "stop": 4.172,
                                "byte_start": 73657,
                                "byte_stop": 90375,
                                "text": "I have disposition of two cases to announce."
                            },
                            {
                                "start": 4.172,
                                "stop": 15.823,
                                "byte_start": 90375,
                                "byte_stop": 136978,
                                "text": "In 75-44 Burrell against McRay, the Writ of Certiorari can be United States Court of Appeals for the Fourth Circuit is dismissed as having been improvidently granted."
                            },
                            {
                                "start": 15.823,
                                "stop": 19.752,
                                "byte_start": 136978,
                                "byte_stop": 152756,
                                "text": "Mr. Justice White would affirm the judgment of Court of Appeals."
                            },
                            {
                                "start": 19.752,
                                "stop": 22.344,
                                "byte_start": 152756,
                                "byte_stop": 163100,
                                "text": "Mr. Justice Stevens filed a concurring opinion."
                            },
                            {
                                "start": 22.344,
                                "stop": 28.063,
                                "byte_start": 163100,
                                "byte_stop": 185879,
                                "text": "Mr. Justice Brennan joined by and Mr. Justice Marshall filed a dissenting opinion."
                            }
                        ]
                    }
